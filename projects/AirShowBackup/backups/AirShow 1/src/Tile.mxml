<?xml version="1.0" encoding="utf-8"?>
<mx:Canvas xmlns:mx="http://www.adobe.com/2006/mxml" width="100" height="100" 
	fontSize="24" backgroundColor="#525252" color="#FFFFFF" horizontalScrollBarStyleName="off" verticalScrollBarStyleName="off">
	<mx:Script>
		<![CDATA[
			[Bindable] public var text:String;
		]]>
	</mx:Script>
	<mx:Label horizontalCenter="0" verticalCenter="0" id="textUI" text="{text}" />
	
</mx:Canvas>
