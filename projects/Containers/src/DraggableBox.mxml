<?xml version="1.0" encoding="utf-8"?>
<HBox xmlns="http://www.adobe.com/2006/mxml" width="100" height="100" paddingLeft="10" paddingBottom="10" paddingRight="10" paddingTop="10"
	mouseDown="dmd(event)">
	<Script>
		<![CDATA[
			import mx.managers.DragManager;
			import mx.core.DragSource;
			private function dmd(event:MouseEvent):void
			{
				DragManager.doDrag(this,new DragSource(),event);
			}
		]]>
	</Script>
	
</HBox>
